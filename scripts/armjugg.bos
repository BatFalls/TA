JumpControl(Func_Var_1)
{
	Static_Var_8 = Func_Var_1;
}

Jumping()
{
	while( TRUE )
	{
		if( Static_Var_8 < 50 )
		{
			emit-sfx 1024 + 0 from l_jetpk;
			emit-sfx 1024 + 0 from r_jetpk;
			sleep 444;
		}
		sleep 1200;
	}
	sleep 1200;
}

BeginJump()
{
	bMoving = FALSE;
	signal SIG_MOVE;
	set-signal-mask SIG_MOVE;
	start-script Jumping();
}

